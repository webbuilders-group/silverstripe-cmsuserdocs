{
    "name": "webbuilders-group/silverstripe-cmsuserdocs",
    "description": "A wrapper for silverstripe/docsviewer that pulls the documentation into the CMS adding a Site Help option to the help menu.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "help", "documentation", "guide"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "https://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/recipe-core": "~4.3",
        "silverstripe/admin": "~1.3",
		"silverstripe/docsviewer": "~3.0"
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-cmsuserdocs/issues"
    },
    "extra": {
        "expose": [
            "css",
            "javascript",
            "images",
            "thirdparty"
        ]
    }
}

